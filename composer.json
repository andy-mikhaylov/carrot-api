{
  "name": "veksa/carrot-api",
  "description": "PHP Wrapper for Carrot Quest API",
  "keywords": [
    "php",
    "carrot-quest",
    "carrot",
    "carrot api"
  ],
  "homepage": "https://github.com/veksa/carrot-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Khijnij",
      "email": "admin@devblog.pro",
      "homepage": "https://devblog.pro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Veksa\\Carrot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Veksa\\Carrot\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}